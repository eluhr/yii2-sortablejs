{
    "name": "eluhr/yii2-sortablejs",
    "description": "Yii2 Wrapper for SortableJS/Sortable",
    "type": "library",
    "authors": [
        {
            "name": "Elias Luhr",
            "email": "elias.luhr@gmail.com"
        }
    ],
    "require": {
        "npm-asset/sortablejs": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "eluhr\\sortablejs\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "npm-asset/sortablejs",
                "version": "1.8.1",
                "dist": {
                    "url": "https://registry.npmjs.org/sortablejs/-/sortablejs-1.8.1.tgz",
                    "type": "tar"
                }
            }
        }
    ]
}
